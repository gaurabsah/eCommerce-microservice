package com.cart;

import jakarta.validation.constraints.NotNull;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class CartItemDTO {

	@NotNull
	private Long userId;

	@NotNull
	private Long productId;

	@NotNull
	private int quantity;

}
